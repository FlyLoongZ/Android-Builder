[
    {
      "kernelSource": {
        "name": "pbem00-kernel",
        "repo": "https://gitlab.com/FlyLoongZhuo/sdm670-mainline-kernel.git",
        "branch": "on-stable",
        "device": "pbem00",
        "defconfig": "pbem00_defconfig"
      },
      "enableCcache": true,
      "toolchains": [
        {
          "name": "clang",
          "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/android12--mainline-release/clang-r416183b1.tar.gz",
          "binaryEnv": [
            "bin"
          ]
        }
      ],
      "params": {
        "ARCH": "arm64",
        "CC": "clang",
        "externalCommands": {
            "LLVM": "1",
            "LLVM_IAS": "1"
          }
      },
      "AnyKernel3": {
        "use": true,
        "release": true
      },
      "enableKernelSU": flase,
      "KernelSUversion": "-",
      "enableLXC": false
    }
] 
